using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _10.VowelsSum
{
    class vowelsSum
    {
        static void Main(string[] args)
        {
            string text = Console.ReadLine();
            int sum = 0;
            for (int curent = 0; curent < text.Length; curent++)
            {
                char symbol = text[curent];
                switch (symbol)
                {
                    case 'a': sum += 1;break;
                    case 'e': sum += 2;break;
                    case 'i': sum += 3;break;
                    case 'o': sum += 4;break;
                    case 'u': sum += 5;break;

                    default: break;
                }
            }
            Console.WriteLine(sum);
        }
    }
}
